{
  "cli": {
    "version": ">= 9.0.3"
  },
  "build": {
    "production": {
      "env": {
        "EXPO_PUBLIC_API_URL": "https://api.production.com"
      },
      "channel": "production"
    },
    "test": {
      "env": {
        "EXPO_PUBLIC_API_URL": "https://api.test.com"
      },
      "channel": "test"
    },
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "preview"
    },
    "preview": {
      "android": {
        "buildType": "apk"
      },
      "channel": "preview"
    }
  },
  "submit": {
    "production": {}
  }
}
